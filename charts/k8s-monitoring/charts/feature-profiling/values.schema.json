{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "deployAsConfigMap": {
            "type": "boolean"
        },
        "ebpf": {
            "type": "object",
            "properties": {
                "demangle": {
                    "type": "string",
                    "enum": [
                        "none",
                        "simplified",
                        "templates",
                        "full"
                    ]
                },
                "enabled": {
                    "type": "boolean"
                },
                "excludeNamespaces": {
                    "type": "array"
                },
                "extraDiscoveryRules": {
                    "type": "string"
                },
                "namespaces": {
                    "type": "array"
                }
            }
        },
        "fullnameOverride": {
            "type": "string"
        },
        "java": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "excludeNamespaces": {
                    "type": "array"
                },
                "extraDiscoveryRules": {
                    "type": "string"
                },
                "namespaces": {
                    "type": "array"
                },
                "profilingConfig": {
                    "type": "object",
                    "properties": {
                        "alloc": {
                            "type": "string"
                        },
                        "cpu": {
                            "type": "boolean"
                        },
                        "interval": {
                            "type": "string"
                        },
                        "lock": {
                            "type": "string"
                        },
                        "sampleRate": {
                            "type": "integer"
                        }
                    }
                }
            }
        },
        "nameOverride": {
            "type": "string"
        },
        "pprof": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "excludeNamespaces": {
                    "type": "array"
                },
                "extraDiscoveryRules": {
                    "type": "string"
                },
                "namespaces": {
                    "type": "array"
                },
                "types": {
                    "type": "object",
                    "properties": {
                        "block": {
                            "type": "boolean"
                        },
                        "cpu": {
                            "type": "boolean"
                        },
                        "fgprof": {
                            "type": "boolean"
                        },
                        "godeltaprof_block": {
                            "type": "boolean"
                        },
                        "godeltaprof_memory": {
                            "type": "boolean"
                        },
                        "godeltaprof_mutex": {
                            "type": "boolean"
                        },
                        "goroutine": {
                            "type": "boolean"
                        },
                        "memory": {
                            "type": "boolean"
                        },
                        "mutex": {
                            "type": "boolean"
                        }
                    }
                }
            }
        }
    }
}
