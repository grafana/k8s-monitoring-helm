{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "global": {
            "type": "object",
            "properties": {
                "maxCacheSize": {
                    "type": "integer"
                },
                "scrapeClassicHistograms": {
                    "type": "boolean"
                },
                "scrapeInterval": {
                    "type": "string"
                },
                "scrapeProtocols": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "scrapeTimeout": {
                    "type": "string"
                }
            }
        },
        "mysql": {
            "type": "object",
            "properties": {
                "instances": {
                    "type": "array"
                }
            }
        },
        "nodeLabels": {
            "type": "object",
            "properties": {
                "availabilityZone": {
                    "type": "boolean"
                },
                "instanceType": {
                    "type": "boolean"
                },
                "nodeArchitecture": {
                    "type": "boolean"
                },
                "nodeOS": {
                    "type": "boolean"
                },
                "nodePool": {
                    "type": "boolean"
                },
                "nodeRole": {
                    "type": "boolean"
                },
                "region": {
                    "type": "boolean"
                }
            }
        },
        "postgresql": {
            "type": "object",
            "properties": {
                "instances": {
                    "type": "array"
                }
            }
        }
    },
    "definitions": {
        "mysql-database": {
            "type": "object",
            "properties": {
                "dataSource": {
                    "type": "object",
                    "properties": {
                        "auth": {
                            "type": "object",
                            "properties": {
                                "password": {
                                    "type": "string"
                                },
                                "passwordFrom": {
                                    "type": "string"
                                },
                                "passwordKey": {
                                    "type": "string"
                                },
                                "username": {
                                    "type": "string"
                                },
                                "usernameFrom": {
                                    "type": "string"
                                },
                                "usernameKey": {
                                    "type": "string"
                                }
                            }
                        },
                        "host": {
                            "type": "string"
                        },
                        "port": {
                            "type": "integer"
                        },
                        "protocol": {
                            "type": "string"
                        },
                        "rawString": {
                            "type": "string"
                        }
                    }
                },
                "exporter": {
                    "type": "object",
                    "properties": {
                        "collectors": {
                            "type": "object",
                            "properties": {
                                "heartbeat": {
                                    "type": "object",
                                    "properties": {
                                        "enabled": {
                                            "type": "boolean"
                                        }
                                    }
                                },
                                "mysqlUser": {
                                    "type": "object",
                                    "properties": {
                                        "enabled": {
                                            "type": "boolean"
                                        },
                                        "privileges": {
                                            "type": "boolean"
                                        }
                                    }
                                }
                            }
                        },
                        "enabled": {
                            "type": "boolean"
                        }
                    }
                },
                "jobLabel": {
                    "type": "string"
                },
                "logs": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "labelSelectors": {
                            "type": "object"
                        },
                        "namespaces": {
                            "type": "array"
                        }
                    }
                },
                "metrics": {
                    "type": "object",
                    "properties": {
                        "extraMetricProcessingRules": {
                            "type": "string"
                        },
                        "maxCacheSize": {
                            "type": "null"
                        },
                        "scrapeInterval": {
                            "type": "null"
                        },
                        "scrapeTimeout": {
                            "type": "string"
                        },
                        "tuning": {
                            "type": "object",
                            "properties": {
                                "excludeMetrics": {
                                    "type": "array"
                                },
                                "includeMetrics": {
                                    "type": "array"
                                }
                            }
                        }
                    }
                },
                "name": {
                    "type": "string"
                },
                "queryAnalysis": {
                    "type": "object",
                    "properties": {
                        "allowUpdatePerformanceSchemaSettings": {
                            "type": "boolean"
                        },
                        "collectors": {
                            "type": "object",
                            "properties": {
                                "explainPlans": {
                                    "type": "object",
                                    "properties": {
                                        "collectInterval": {
                                            "type": "string"
                                        },
                                        "enabled": {
                                            "type": "boolean"
                                        },
                                        "excludeSchemas": {
                                            "type": "array"
                                        },
                                        "initialLookback": {
                                            "type": "string"
                                        },
                                        "perCollectRatio": {
                                            "type": "number"
                                        }
                                    }
                                },
                                "locks": {
                                    "type": "object",
                                    "properties": {
                                        "collectInterval": {
                                            "type": "string"
                                        },
                                        "enabled": {
                                            "type": "boolean"
                                        },
                                        "threshold": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "queryDetails": {
                                    "type": "object",
                                    "properties": {
                                        "enabled": {
                                            "type": "boolean"
                                        }
                                    }
                                },
                                "querySamples": {
                                    "type": "object",
                                    "properties": {
                                        "autoEnableSetupConsumers": {
                                            "type": "boolean"
                                        },
                                        "collectInterval": {
                                            "type": "string"
                                        },
                                        "disableQueryRedaction": {
                                            "type": "boolean"
                                        },
                                        "enabled": {
                                            "type": "boolean"
                                        },
                                        "setupConsumersCheckInterval": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "schemaDetails": {
                                    "type": "object",
                                    "properties": {
                                        "cacheEnabled": {
                                            "type": "boolean"
                                        },
                                        "cacheSize": {
                                            "type": "integer"
                                        },
                                        "cacheTTL": {
                                            "type": "string"
                                        },
                                        "collectInterval": {
                                            "type": "string"
                                        },
                                        "enabled": {
                                            "type": "boolean"
                                        }
                                    }
                                },
                                "setupConsumers": {
                                    "type": "object",
                                    "properties": {
                                        "collectInterval": {
                                            "type": "string"
                                        },
                                        "enabled": {
                                            "type": "boolean"
                                        }
                                    }
                                }
                            }
                        },
                        "enabled": {
                            "type": "boolean"
                        }
                    }
                },
                "secret": {
                    "type": "object",
                    "properties": {
                        "create": {
                            "type": "null"
                        },
                        "embed": {
                            "type": "boolean"
                        },
                        "name": {
                            "type": "string"
                        },
                        "namespace": {
                            "type": "string"
                        }
                    }
                },
                "type": {
                    "type": "string",
                    "const": "mysql"
                }
            }
        }
    }
}
